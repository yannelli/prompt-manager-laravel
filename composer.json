{
    "name": "yannelli/prompt-manager-laravel",
    "description": "A Laravel package for managing prompt templates with versioning, component toggling, chaining, and customizable type handlers",
    "keywords": [
        "laravel",
        "prompts",
        "templates",
        "ai",
        "llm",
        "versioning"
    ],
    "homepage": "https://github.com/yannelli/prompt-manager-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Yannelli",
            "email": "ryanyannelli@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/pipeline": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Yannelli\\PromptManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yannelli\\PromptManager\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "echo 'Package ready'",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yannelli\\PromptManager\\PromptManagerServiceProvider"
            ],
            "aliases": {
                "PromptManager": "Yannelli\\PromptManager\\Facades\\PromptManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
